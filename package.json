{
  "name": "root",
  "version": "0.0.1",
  "packageManager": "pnpm@7.3.0",
  "private": false,
  "description": "element-plus和font-awesome图标选择器",
  "license": "MIT",
  "author": "cnovel.top",
  "keywords": [
    "vue",
    "icon-picker",
    "e-icon-picker",
    "iconPicker",
    "eiconPicker",
    "icon picker",
    "icon"
  ],
  "homepage": "https://e-icon-picker-ts.cnovel.top",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/cnovel/e-icon-picker"
  },
  "bugs": {
    "url": "https://gitee.com/cnovel/e-icon-picker/issues"
  },
  "main": "index.umd.js",
  "module": "index.mjs",
  "unpkg": "index.umd.js",
  "jsdelivr": "index.umd.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./*": "./*"
  },
  "style": "index.css",
  "sideEffects": [
    "theme/*",
    "components/*/*"
  ],
  "scripts": {
    "build:lib": "pnpm clean:lib && pnpm build:components && pnpm release",
    "build:components": "tsx scripts/build.ts",
    "build:icon": "pnpm clean:icon && tsx scripts/build-icon.ts",
    "clean:icon": "cd ./packages/icon && rimraf ./antd ./arco ./ele ./fontawesome ./layui-vue ./tdesign ./iconify",
    "release": "tsx scripts/release.ts",
    "clean:lib": "rimraf lib",
    "update:version": "tsx scripts/update-version.ts",
    "publish": "npm publish ./lib --tag next",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:preview": "pnpm run -C docs preview",
    "eslint": "eslint \"packages/**/{*.vue,*.ts}\"",
    "eslint:fix": "eslint --fix \"packages/**/{*.vue,*.ts}\"",
    "stylelint": "stylelint \"packages/**/{*.scss,*.css}\"",
    "stylelint:fix": "stylelint --fix \"packages/**/{*.scss,*.css}\""
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vue/shared": "^3.4.15",
    "@vueuse/core": "^10.7.2",
    "debounce": "^2.0.0",
    "vue": "^3.4.15",
    "@iconify/vue": "^4.1.1",
    "e-icon-picker": "workspace:*"
  },
  "devDependencies": {
    "@layui/icons-vue": "^1.1.0",
    "@ant-design/icons-vue": "^7.0.1",
    "@arco-design/web-vue": "^2.54.3",
    "@babel/parser": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/traverse": "^7.23.9",
    "@element-plus/icons-vue": "^2.3.1",
    "@rollup/pluginutils": "^5.1.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/debounce": "^1.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.7",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.20.1",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "shelljs": "^0.8.5",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.1.0",
    "tdesign-icons-vue-next": "^0.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vue-tsc": "^1.8.27",
    "@iconify/json": "^2.2.177"
  },
  "lint-staged": {
    "packages/**/{*.vue,*.ts}": "eslint --fix",
    "packages/**/{*.scss,*.css}": "stylelint --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
