{
  "name": "root",
  "version": "0.0.1",
  "packageManager": "pnpm@7.3.0",
  "private": false,
  "description": "element-plus和font-awesome图标选择器",
  "license": "MIT",
  "author": "cnovel.club",
  "keywords": [
    "vue",
    "icon-picker",
    "e-icon-picker",
    "iconPicker",
    "eiconPicker",
    "icon picker",
    "icon"
  ],
  "homepage": "https://e-icon-picker-ts.cnovel.club",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/cnovel/e-icon-picker"
  },
  "bugs": {
    "url": "https://gitee.com/cnovel/e-icon-picker/issues"
  },
  "workspaces": [
    "packages/*",
    "docs",
    "./**"
  ],
  "main": "index.umd.js",
  "module": "index.mjs",
  "unpkg": "index.umd.js",
  "jsdelivr": "index.umd.js",
  "style": "index.css",
  "types": "index.d.ts",
  "scripts": {
    "build:lib": "pnpm clean:lib && pnpm build:components && pnpm build:icon && pnpm release",
    "build:components": "tsx scripts/build.ts",
    "build:icon": "pnpm clean:icon && tsx scripts/build-icon.ts",
    "clean:icon": "rimraf ./scripts/icon/ele && rimraf ./scripts/icon/fontawesome",
    "release": "tsx scripts/release.ts",
    "clean:lib": "rimraf lib",
    "update:version": "tsx scripts/update-version.ts",
    "publish": "npm publish ./lib --tag next",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:preview": "pnpm run -C docs preview",
    "eslint": "eslint \"packages/**/{*.vue,*.ts}\"",
    "eslint:fix": "eslint --fix \"packages/**/{*.vue,*.ts}\"",
    "stylelint": "stylelint \"packages/**/{*.scss,*.css}\"",
    "stylelint:fix": "stylelint --fix \"packages/**/{*.scss,*.css}\""
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@vue/shared": "^3.2.45",
    "@vueuse/core": "^9.9.0",
    "debounce": "^1.2.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@babel/parser": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/traverse": "^7.20.10",
    "@element-plus/icons-vue": "^2.0.10",
    "@arco-design/web-vue": "^2.40.1",
    "@rollup/pluginutils": "^5.0.2",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/debounce": "^1.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.17",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.8.0",
    "fs-extra": "^11.1.0",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "shelljs": "^0.8.5",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "tdesign-icons-vue-next": "^0.1.7",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.17"
  },
  "lint-staged": {
    "packages/**/{*.vue,*.ts}": "eslint --fix",
    "packages/**/{*.scss,*.css}": "stylelint --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
