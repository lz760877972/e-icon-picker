    "vite": "^3.2.4",
    "vite-plugin-banner": "^0.6.1",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-md": "^0.20.4",
    "vitepress": "1.0.0-alpha.29",
    "vitepress-theme-demoblock": "2.0.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.9",
    "chalk": "^4.1.2"
