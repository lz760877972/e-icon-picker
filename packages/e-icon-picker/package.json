    "@types/prompts": "^2.4.1",
    "@types/chalk": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "commander": "^9.4.1",
    "element-plus": "^2.2.22",
    "esbuild-register": "^3.4.1",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
