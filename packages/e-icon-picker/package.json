{
  "name": "e-icon-picker",
  "version": "2.1.0",
  "description": "element-plus和font-awesome图标选择器",
  "license": "MIT",
  "author": "cnovel.club",
  "keywords": [
    "vue",
    "icon-picker",
    "e-icon-picker",
    "iconPicker",
    "eiconPicker",
    "icon picker",
    "icon"
  ],
  "homepage": "https://e-icon-picker.cnovel.club",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/cnovel/e-icon-picker"
  },
  "bugs": {
    "url": "https://gitee.com/cnovel/e-icon-picker/issues"
  },
  "main": "index.umd.js",
  "module": "index.mjs",
  "unpkg": "index.umd.js",
  "jsdelivr": "index.umd.js",
  "style": "index.css",
  "types": "index.d.ts",
  "scripts": {
    "build:lib": "pnpm clean:lib && pnpm build:components && pnpm build:icon && pnpm release",
    "build:components": "node ./cli/index.js build",
    "build:icon": "pnpm clean:icon && node ./cli/index.js icon",
    "clean:icon": "rimraf ./cli/commands/icon/ele && rimraf ./cli/commands/icon/fontawesome",
    "release": "node ./cli/index.js release",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "clean:lib": "rimraf lib"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@vue/shared": "^3.2.45",
    "@vueuse/core": "^9.5.0",
